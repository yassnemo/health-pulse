# HealthPulse Analytics Frontend Environment Variables Example
# Copy this file to .env and fill in your values

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_API_TIMEOUT=30000

# Authentication
REACT_APP_AUTH_STORAGE_KEY=healthpulse_auth_token

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_DEMO_MODE=true

# UI Configuration
REACT_APP_MAX_PATIENTS_PER_PAGE=20
REACT_APP_DEFAULT_THEME=light

# Integration Services
REACT_APP_ML_SERVICE_URL=http://localhost:5000

# Logging and Monitoring
REACT_APP_LOG_LEVEL=info
REACT_APP_TELEMETRY_ENABLED=false

# Development Options
REACT_APP_DEV_BYPASS_AUTH=false
